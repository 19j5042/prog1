def emoji(eye):
    print("(",end="")
    print(eye,end="")
    print("_",end="")
    print(eye,end="")
    print(")",end="")
def emoji_table(m,n):
    for y in range(1,n+1):
        for x in range(1,m+1):
            if((y+x)%2!=0):
                emoji("*")
            else:
                emoji("-")
        print()

emoji_table(8,4)
